{
  "name": "mist-signer",
  "version": "0.3.0",
  "description": "Mist Signer",
  "main": "dist",
  "scripts": {
    "dev": "nodemon -w src --exec \"babel-node src --presets es2015,stage-0\"",
    "build": "babel src -s -D -d dist --presets es2015,stage-0",
    "start": "node dist",
    "prestart": "npm run -s build",
    "lint": "eslint --fix --ext .js,.ts src",
    "test": "eslint src",
    "bot": "nodemon -w src/bot --exec \"babel-node src/bot --presets es2015,stage-0\"",
    "ws": "source mist_product.env && nodemon -w src/ws --exec \"babel-node src/ws --presets es2015,stage-0\"",
    "bridge": "nodemon -w src/did/bridge --exec \"babel-node src/did/bridge --presets es2015,stage-0\"",
    "server-dev": "source ./mist_test.env && babel-node src --presets es2015,stage-0",
    "server-product": "source ./mist_product.env && babel-node src --presets es2015,stage-0",
    "pm2":"npm run -s build && source ./mist_product.env && pm2 start dist"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "repository": "mistdex/mist-signer",
  "author": "Ryan",
  "license": "MIT",
  "dependencies": {
    "@asimovdev/asimovjs": "1.0.30",
    "@spinlee/ethers": "^1.0.12",
    "await-to-js": "^2.1.1",
    "axios": "^0.19.0",
    "bcrypt-nodejs": "^0.0.3",
    "bignumber.js": "^8.0.2",
    "bip32": "^2.0.4",
    "bip39": "^2.6.0",
    "bitcoinjs-lib": "^5.1.6",
    "bitcore-lib": "^0.15.0",
    "bitcore-message": "^1.0.4",
    "body-parser": "^1.13.3",
    "bs58": "^4.0.1",
    "bs58check": "^2.1.2",
    "chromedriver": "^75.0.0",
    "clipboard": "^2.0.4",
    "compression": "^1.5.2",
    "cors": "^2.7.1",
    "crypto-js": "^3.1.9-1",
    "element-ui": "^2.4.11",
    "ethereumjs-abi": "^0.6.8",
    "ethereumjs-util": "^6.1.0",
    "ethereumjs-wallet": "^0.6.3",
    "ethers": "^4.0.38",
    "express": "^4.13.3",
    "extensionizer": "^1.0.1",
    "jquery": "^3.3.1",
    "js-base64": "^2.5.1",
    "js-cookie": "^2.2.0",
    "js-sha3": "^0.8.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.4",
    "md5": "^2.2.1",
    "moment": "^2.24.0",
    "mongoose": "^5.7.3",
    "morgan": "^1.8.0",
    "nodemailer": "^6.3.1",
    "nprogress": "^0.2.0",
    "number-precision": "^1.3.2",
    "object-hash": "^2.0.0",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "pg": "^7.12.1",
    "qrcode.vue": "^1.6.1",
    "qs": "^6.6.0",
    "resource-router-middleware": "^0.6.0",
    "scryptsy": "^2.0.0",
    "silly-datetime": "^0.1.2",
    "solc": "^0.5.11",
    "uuid": "^3.3.2",
    "web3": "^1.2.2",
    "websocket": "^1.0.30"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.9.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^3.1.1",
    "nodemon": "^1.9.2"
  }
}
