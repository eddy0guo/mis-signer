{
  "name": "mist-signer",
  "version": "0.3.0",
  "description": "Mist Signer",
  "main": "dist",
  "scripts": {
    "dev": "nodemon -w src --exec \"babel-node src\"",
    "build": "babel src -s -D -d dist",
    "start": "node dist",
    "prestart": "npm run -s build",
    "lint": "eslint --fix --ext .js,.ts src",
    "test": "jest",
    "bot": "nodemon -w src/bot --exec \"babel-node src/bot\"",
    "engine-local": "source ./local.env && node dist/adex_engine",
    "engine-dev": "source ./dev.env && node dist/adex_engine",
    "engine-product": "source ./product.env && node dist/adex_engine",
    "watcher-local": "source ./local.env && node dist/adex_watcher",
    "watcher-dev": "source ./dev.env && node dist/adex_watcher",
    "watcher-product": "source ./product.env && node dist/adex_watcher",
    "laucher-local": "source ./local.env && node dist/adex_laucher",
    "laucher-dev": "source ./dev.env && node dist/adex_laucher",
    "laucher-product": "source ./product.env && node dist/adex_laucher",
    "express-local": "source ./local.env && node dist/express_watcher",
    "express-dev": "source ./dev.env && node dist/express_watcher",
    "express-product": "source ./product.env && node dist/express_watcher",
    "bridge-local": "source ./local.env && node dist/bridge_watcher",
    "bridge-dev": "source ./dev.env && node dist/bridge_watcher",
    "bridge-product": "source ./product.env && node dist/bridge_watcher",
    "bridge": "nodemon -w src/did/bridge --exec \"babel-node src/did/bridge\"",
    "server-local": "source ./local.env && babel-node src",
    "server-dev": "source ./dev.env && babel-node src",
    "server-product": "source ./product.env && babel-node src",
    "sdk-test": "nodemon -w src/test --exec \"babel-node src/test\"",
    "doc": "npx compodoc -p tsconfig.json"
  },
  "repository": "mistdex/mist-signer",
  "author": "Ryan",
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-commit": "eslint --fix --ext .js,.ts src"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
