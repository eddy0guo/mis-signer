{
  "name": "mist-signer",
  "version": "0.3.0",
  "description": "Mist Signer",
  "main": "dist",
  "scripts": {
    "dev": "nodemon -w src --exec \"babel-node src --presets es2015,stage-0\"",
    "build": "babel src -s -D -d dist --presets es2015,stage-0",
    "start": "node dist",
    "prestart": "npm run -s build",
    "test": "eslint src"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "repository": "mistdex/mist-signer",
  "author": "Ryan",
  "license": "MIT",
  "dependencies": {
    "@asimovdev/asimovjs": "1.0.30",
    "@spinlee/ethers": "^1.0.12",
    "await-to-js": "^2.1.1",
    "axios": "^0.17.1",
    "bignumber.js": "^8.0.2",
    "bip39": "^2.5.0",
    "bitcore-lib": "^0.15.0",
    "bitcore-message": "^1.0.4",
    "body-parser": "^1.13.3",
    "bs58": "^4.0.1",
    "bs58check": "^2.1.2",
    "chromedriver": "^75.0.0",
    "clipboard": "^2.0.4",
    "compression": "^1.5.2",
    "cors": "^2.7.1",
    "crypto-js": "^3.1.9-1",
    "element-ui": "^2.4.11",
    "ethers": "^4.0.27",
    "express": "^4.13.3",
    "extensionizer": "^1.0.1",
    "jquery": "^3.3.1",
    "js-base64": "^2.5.1",
    "js-cookie": "^2.2.0",
    "js-sha3": "^0.8.0",
    "lodash": "^4.17.4",
    "md5": "^2.2.1",
    "moment": "^2.24.0",
    "morgan": "^1.8.0",
    "nprogress": "^0.2.0",
    "pg": "^7.12.1",
    "qrcode.vue": "^1.6.1",
    "qs": "^6.6.0",
    "resource-router-middleware": "^0.6.0",
    "scryptsy": "^2.0.0",
    "solc": "^0.5.11",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.9.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^3.1.1",
    "nodemon": "^1.9.2"
  }
}
