{
  "name": "mist-signer",
  "version": "0.3.0",
  "description": "Mist Signer",
  "main": "dist",
  "scripts": {
    "dev": "nodemon -w src --exec \"babel-node src --presets es2015,stage-0\"",
    "build": "babel src -s -D -d dist --presets es2015,stage-0",
    "start": "node dist",
    "prestart": "npm run -s build",
    "lint": "eslint --fix --ext .js,.ts src",
    "test": "eslint src",
    "bot": "nodemon -w src/bot --exec \"babel-node src/bot --presets es2015,stage-0\"",
    "ws": "source ./product.env && nodemon -w src/ws --exec \"babel-node src/ws --presets es2015,stage-0\"",
    "engine-local": "source ./local.env && nodemon -w src/adex_engine --exec \"babel-node src/adex_engine --presets es2015,stage-0\"",
    "engine-dev": "source ./dev.env && nodemon -w src/adex_engine --exec \"babel-node src/adex_engine --presets es2015,stage-0\"",
    "engine-product": "source ./product.env && nodemon -w src/adex_engine --exec \"babel-node src/adex_engine --presets es2015,stage-0\"",
    "watcher-local": "source ./local.env && nodemon -w src/adex_watcher --exec \"babel-node src/adex_watcher --presets es2015,stage-0\"",
    "watcher-dev": "source ./dev.env && nodemon -w src/adex_watcher --exec \"babel-node src/adex_watcher --presets es2015,stage-0\"",
    "watcher-product": "source ./product.env && nodemon -w src/adex_watcher --exec \"babel-node src/adex_watcher --presets es2015,stage-0\"",

    "laucher-local": "source ./local.env && nodemon -w src/adex_laucher --exec \"babel-node src/adex_laucher --presets es2015,stage-0\"",
    "laucher-dev": "source ./dev.env && nodemon -w src/adex_laucher --exec \"babel-node src/adex_laucher --presets es2015,stage-0\"",
    "laucher-product": "source ./product.env && nodemon -w src/adex_laucher --exec \"babel-node src/adex_laucher --presets es2015,stage-0\"",


    "ws-poa": "source ./dev.env && nodemon -w src/ws --exec \"babel-node src/ws --presets es2015,stage-0\"",
    "bridge": "nodemon -w src/did/bridge --exec \"babel-node src/did/bridge --presets es2015,stage-0\"",
    "server-local": "source ./local.env && babel-node src --presets es2015,stage-0",
    "server-dev": "source ./dev.env && babel-node src --presets es2015,stage-0",
    "server-product": "source ./product.env && babel-node src --presets es2015,stage-0",
    "sdk-test": "nodemon -w src/test --exec \"babel-node src/test --presets es2015,stage-0\""
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "repository": "mistdex/mist-signer",
  "author": "Ryan",
  "license": "MIT",
  "dependencies": {
    "@alicloud/sms-sdk": "^1.1.6",
    "@asimovdev/asimovjs": "^1.0.121",
    "@spinlee/ethers": "^1.0.12",
    "apicache": "^1.5.2",
    "asimov-wallet": "^1.0.21",
    "await-to-js": "^2.1.1",
    "axios": "^0.19.0",
    "bcrypt-nodejs": "^0.0.3",
    "bignumber.js": "^8.0.2",
    "bip32": "^2.0.4",
    "bip39": "^2.6.0",
    "bitcoinjs-lib": "^5.1.6",
    "bitcore-lib": "=8.1.0",
    "bitcore-message": "^1.0.4",
    "body-parser": "^1.13.3",
    "bs58": "^4.0.1",
    "bs58check": "^2.1.2",
    "bull": "^3.12.1",
    "chromedriver": "^75.0.0",
    "clipboard": "^2.0.4",
    "compression": "^1.5.2",
    "consola": "^2.11.0",
    "cors": "^2.7.1",
    "crypto-js": "^3.1.9-1",
    "element-ui": "^2.4.11",
    "ethereumjs-abi": "^0.6.8",
    "ethereumjs-util": "^6.1.0",
    "ethereumjs-wallet": "^0.6.3",
    "ethers": "^4.0.38",
    "express": "^4.13.3",
    "extensionizer": "^1.0.1",
    "jquery": "^3.3.1",
    "js-base64": "^2.5.1",
    "js-cookie": "^2.2.0",
    "js-sha3": "^0.8.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.4",
    "log4js": "^6.1.0",
    "md5": "^2.2.1",
    "moment": "^2.24.0",
    "mongoose": "^5.7.3",
    "morgan": "^1.8.0",
    "nodemailer": "^6.3.1",
    "nprogress": "^0.2.0",
    "number-precision": "^1.3.2",
    "object-hash": "^2.0.0",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "pg": "^7.12.1",
    "postgres-pool": "^2.0.2",
    "qrcode.vue": "^1.6.1",
    "qs": "^6.6.0",
    "resource-router-middleware": "^0.6.0",
    "scryptsy": "^2.0.0",
    "silly-datetime": "^0.1.2",
    "solc": "^0.5.11",
    "uuid": "^3.3.2",
    "web3": "^0.20.7",
    "websocket": "^1.0.30"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.9.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^3.1.1",
    "ethereumjs-tx": "^2.1.1",
    "nodemon": "^1.9.2"
  }
}
